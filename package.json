{
  "name": "hatenablogger",
  "displayName": "hatenablogger",
  "publisher": "uraway",
  "license": "MIT",
  "description": "Post an entry to hatenablog via vscode",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uraway/hatenablogger.git"
  },
  "bugs": {
    "url": "https://github.com/uraway/hatenablogger/issues"
  },
  "version": "0.1.12",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.postOrUpdate",
        "title": "Hatenablogger: Post or Update"
      },
      {
        "command": "extension.uploadImage",
        "title": "Hatenablogger: Upload Image"
      }
    ],
    "configuration": {
      "title": "Hatenablogger configuration",
      "properties": {
        "hatenablogger.hatenaId": {
          "type": "string"
        },
        "hatenablogger.blogId": {
          "type": "string"
        },
        "hatenablogger.apiKey": {
          "type": "string"
        },
        "hatenablogger.fotolifeFolder": {
          "type": "string",
          "default": "Hatena Blog"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -p ./",
    "publish": "vsce publish --yarn",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@types/vscode": "^1.50.0",
    "@types/xml2js": "^0.4.3",
    "all-contributors-cli": "^6.3.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "vscode-test": "^1.4.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "axios": "0.21.0",
    "hatena-fotolife-api": "2.0.0",
    "wsse": "5.1.0",
    "xml2js": "^0.4.19"
  }
}

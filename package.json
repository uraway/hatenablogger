{
  "name": "hatenablogger",
  "displayName": "hatenablogger",
  "publisher": "uraway",
  "description": "Post an entry to hatenablog via vscode",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uraway/hatenablogger.git"
  },
  "bugs": {
    "url": "https://github.com/uraway/hatenablogger/issues"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.postOrUpdate",
        "title": "Hatenablogger: Post or Update"
      },
      {
        "command": "extension.uploadImage",
        "title": "Hatenablogger: Upload Image"
      }
    ],
    "configuration": {
      "title": "Hatenablogger configuration",
      "properties": {
        "hatenablogger.hatenaId": {
          "type": "string"
        },
        "hatenablogger.blogId": {
          "type": "string"
        },
        "hatenablogger.apiKey": {
          "type": "string"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/xml2js": "^0.4.3",
    "axios": "^0.18.0",
    "hatena-blog-api": "^0.3.0",
    "hatena-fotolife-api": "^0.4.1",
    "wsse": "^2.4.0",
    "xml2js": "^0.4.19"
  }
}

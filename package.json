{
  "name": "hatenablogger",
  "displayName": "hatenablogger",
  "publisher": "uraway",
  "license": "MIT",
  "description": "Post an entry to hatenablog via vscode",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uraway/hatenablogger.git"
  },
  "bugs": {
    "url": "https://github.com/uraway/hatenablogger/issues"
  },
  "version": "0.3.3",
  "engines": {
    "vscode": ">= 1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.postOrUpdate",
        "title": "Hatenablogger: Post or Update"
      },
      {
        "command": "extension.uploadImage",
        "title": "Hatenablogger: Upload Image"
      }
    ],
    "configuration": {
      "title": "Hatenablogger configuration",
      "properties": {
        "hatenablogger.hatenaId": {
          "type": "string"
        },
        "hatenablogger.blogId": {
          "type": "string"
        },
        "hatenablogger.apiKey": {
          "type": "string"
        },
        "hatenablogger.fotolifeFolder": {
          "type": "string",
          "default": "Hatena Blog"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./",
    "publish": "vsce publish --yarn",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn compile",
    "test": "node ./out/test/runTest.js",
    "contributors:add": "all-contributors add",
    "lint": "npm-run-all -p lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:type": "tsc -p ./ --noEmit",
    "contributors:generate": "all-contributors generate"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.22",
    "@types/vscode": "1.52.0",
    "@types/xml2js": "0.4.8",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "all-contributors-cli": "6.19.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "glob": "^7.1.6",
    "mocha": "8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "typescript": "4.1.3",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "axios": "0.21.1",
    "hatena-fotolife-api": "2.0.0",
    "wsse": "5.1.0",
    "xml2js": "^0.4.19"
  }
}

{
  "name": "hatenablogger",
  "displayName": "hatenablogger",
  "publisher": "uraway",
  "license": "MIT",
  "description": "Post an entry to hatenablog via vscode",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uraway/hatenablogger.git"
  },
  "bugs": {
    "url": "https://github.com/uraway/hatenablogger/issues"
  },
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.postOrUpdate",
        "title": "Hatenablogger: Post or Update"
      },
      {
        "command": "extension.uploadImage",
        "title": "Hatenablogger: Upload Image"
      }
    ],
    "configuration": {
      "title": "Hatenablogger configuration",
      "properties": {
        "hatenablogger.hatenaId": {
          "type": "string"
        },
        "hatenablogger.blogId": {
          "type": "string"
        },
        "hatenablogger.apiKey": {
          "type": "string"
        },
        "hatenablogger.fotolifeFolder": {
          "type": "string",
          "default": "Hatena Blog"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^12.0.0",
    "all-contributors-cli": "^6.3.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/xml2js": "^0.4.3",
    "axios": "^0.18.1",
    "hatena-fotolife-api": "^1.0.0",
    "wsse": "^2.4.0",
    "xml2js": "^0.4.19"
  }
}
